{%- extends '_layout.njk' %}

{%- block content %}

  <ul class="h-feed">
  {%- for post in posts %}
    <li class="h-entry">
      <a href="{{ post.url }}">
        {% include '_published.njk' %}
      </a>
    </li>
  {%- endfor %}
  </ul>

  {%- if (lastPublishedInt) %}
    <p><a href="?before={{ lastPublishedInt }}" rel="nofollow prev">Previous</a></p>
  {%- endif %}

{%- endblock %}
