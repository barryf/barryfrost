{%- extends 'layout.njk' %}

{%- block head %}
<title>410 Gone</title>
{%- endblock %}

{%- block content %}
<main class="page-content">
  <div class="wrapper">
    <article class="post h-entry">

      <header class="post-header">
        <h1 class="post-title p-name"><code>410</code> Gone</h1>
      </header>

      <div class="post-content e-content">
        <p>This post has been deleted and is no longer available.</p>
      </div>

      <time class="dt-deleted" datetime="{{ post.deleted }}"></time>

    </article>
  </div>
</main>
{%- endblock %}
